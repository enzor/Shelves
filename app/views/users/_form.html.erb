<%= form_tag :controller => 'users', :action => 'new'  %>
<h2>Register</h2>
<p>
    <b> <%= label :username, "Username:" %></b>
    <%= text_field_tag :username %>
</p>
<p>
    <b> <%= label :email, "Email Address:" %></b>
    <%= text_field_tag :email %>
</p>

<p>
  <%= submit_tag 'Proceed', :class => 'button' %>
</p>
